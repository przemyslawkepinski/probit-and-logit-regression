#przygotowanie zmiennej change_weight
# 1 decreased
# 2 constant
# 3 increased

dane$change_weight <- factor(dane$change_weight,
                      levels = c("Decreased",
                                 "Remain Constant",
                                 "Increased"),
                      ordered = TRUE) 

